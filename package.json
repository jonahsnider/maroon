	},
	"dependencies": {
		"@material-ui/core": "4.11.0",
		"@material-ui/icons": "4.9.1",
		"@types/node": "12.12.50",
		"@types/react": "16.9.43",
		"firebase": "7.16.1",
		"next": "9.4.4",
		"next-compose-plugins": "2.2.0",
		"next-pwa": "3.0.3",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"typescript": "3.9.6",
		"ytdl-core": "3.1.2"
	},
	"devDependencies": {
		"eslint-config-xo-react": "0.23.0",
		"eslint-config-xo-typescript": "0.31.0",
		"eslint-plugin-react": "7.20.3",
		"eslint-plugin-react-hooks": "4.0.8",
		"prettier": "2.0.4",
		"xo": "0.32.1"
	},
	"homepage": "https://maroon.jonah.pw",
	"name": "maroon",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"dev:inspect": "NODE_OPTIONS='--inspect' next dev",
		"export": "next export",
		"lint": "xo",
		"preexport": "rm -rf out",
		"start": "next start",
		"style": "prettier --ignore-path .gitignore --check ."
	},
	"version": "3.0.0",
	"xo": {
		"extends": [
			"xo",
			"xo/esnext",
			"xo/browser",
			"xo-react"
		],
		"overrides": [
			{
				"extends": [
					"xo"
				],
				"files": [
					"*.config.js",
					"src/config.ts",
					"src/pages/api/*"
				]
			}
		],
		"prettier": true,
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"firebase/*",
						"firebase"
					]
				}
			]
		}
	}
}
