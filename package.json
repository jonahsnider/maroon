	},
	"dependencies": {
		"@material-ui/core": "4.10.2",
		"@material-ui/icons": "4.9.1",
		"@types/node": "12.12.47",
		"@types/react": "16.9.36",
		"firebase": "7.15.0",
		"fs-extra": "9.0.1",
		"next": "9.4.4",
		"next-compose-plugins": "2.2.0",
		"next-pwa": "2.5.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"typescript": "3.9.3",
		"ytdl-core": "2.1.7"
	},
	"devDependencies": {
		"eslint-config-xo-react": "0.23.0",
		"eslint-config-xo-typescript": "0.31.0",
		"eslint-plugin-react": "7.20.0",
		"eslint-plugin-react-hooks": "4.0.4",
		"prettier": "2.0.4",
		"xo": "0.32.0"
	},
	"homepage": "https://maroon.jonah.pw",
	"name": "maroon",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"dev:inspect": "NODE_OPTIONS='--inspect' next dev",
		"export": "next export",
		"lint": "xo",
		"preexport": "rm -rf out",
		"start": "next start",
		"style": "prettier --ignore-path .gitignore --check ."
	},
	"version": "3.0.0",
	"xo": {
		"extends": [
			"xo",
			"xo/esnext",
			"xo/browser",
			"xo-react"
		],
		"overrides": [
			{
				"extends": [
					"xo"
				],
				"files": [
					"*.config.js",
					"src/config.ts",
					"src/pages/api/*"
				]
			}
		],
		"prettier": true,
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"firebase/*",
						"firebase"
					]
				}
			]
		}
	}
}
