{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"dependencies": {
		"@lazy-http/content-disposition": "1.0.2",
		"@material-ui/core": "4.12.3",
		"@material-ui/icons": "4.11.2",
		"@types/node": "16.11.14",
		"@types/react": "17.0.37",
		"file-type": "16.5.3",
		"firebase": "8.10.0",
		"next": "11.0.1",
		"next-compose-plugins": "2.2.1",
		"next-pwa": "5.4.4",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"typescript": "4.5.4",
		"ytdl-core": "4.9.1"
	},
	"devDependencies": {
		"eslint-config-xo-react": "0.25.0",
		"eslint-plugin-react": "7.27.1",
		"eslint-plugin-react-hooks": "4.2.0",
		"prettier": "2.5.1",
		"prettier-config-xo": "2.0.0",
		"xo": "0.46.4"
	},
	"homepage": "https://maroon.jonah.pw",
	"name": "maroon",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"dev:inspect": "NODE_OPTIONS='--inspect' next dev",
		"export": "next export",
		"lint": "xo",
		"preexport": "rm -rf out",
		"start": "next start",
		"style": "prettier --ignore-path .gitignore --check ."
	},
	"version": "3.0.0",
	"xo": {
		"extends": [
			"xo",
			"xo/browser",
			"xo-react"
		],
		"ignore": [
			"./next-env.d.ts"
		],
		"overrides": [
			{
				"extends": [
					"xo"
				],
				"files": [
					"*.config.js",
					"*.cjs",
					"src/config.ts",
					"src/pages/api/*"
				]
			}
		],
		"prettier": true,
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"firebase/*",
						"firebase"
					]
				}
			],
			"react/no-unescaped-entities": "off",
			"react/prop-types": "off",
			"unicorn/prefer-node-protocol": "off"
		}
	}
}
